<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>org.maxgamer</groupId>
    <artifactId>QuickShop</artifactId>

    <properties>
        <pluginver>5.1.2.1</pluginver>
        <package>org.maxgamer.quickshop</package>
        <developer>Ghost-chu</developer>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>

    <version>${mvn.build.ver}</version>

    <licenses>
        <license>
            <name>GPL v3 License</name>
            <url>https://www.gnu.org/licenses/</url>
            <distribution>repo</distribution>
        </license>
    </licenses>

    <issueManagement>
        <url>https://github.com/${developer}/${name}/issues</url>
        <system>GitHub Issues</system>
    </issueManagement>

    <distributionManagement>
        <repository>
            <id>codemc-releases</id>
            <url>https://repo.codemc.io/repository/maven-releases/</url>
        </repository>
        <snapshotRepository>
            <id>codemc-snapshots</id>
            <url>https://repo.codemc.io/repository/maven-snapshots/</url>
        </snapshotRepository>
    </distributionManagement>

    <profiles>
        <profile>
            <id>debug</id>
            <properties>
                <mvn.build.ver>${pluginver}-SNAPSHOT</mvn.build.ver>
                <!-- Disable signature in local env, deploy phrase never will running in local env -->
                <mvn.sign.phrase>deploy</mvn.sign.phrase>
            </properties>
        </profile>
        <profile>
            <id>master</id>
            <properties>
                <mvn.build.ver>${pluginver}-SNAPSHOT</mvn.build.ver>
                <mvn.sign.phrase>install</mvn.sign.phrase>
            </properties>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
        </profile>
        <profile>
            <id>release</id>
            <properties>
                <mvn.build.ver>${pluginver}</mvn.build.ver>
                <mvn.sign.phrase>install</mvn.sign.phrase>
            </properties>
        </profile>
    </profiles>

    <scm>
        <url>https://github.com/${developer}/${project.version}</url>
        <connection>scm:git:git://github.com/${developer}/${project.version}.git</connection>
        <developerConnection>scm:git:git@github.com:${developer}/${project.version}.git</developerConnection>
    </scm>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-dependency-plugin</artifactId>
                <version>3.3.0</version>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.10.1</version>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                    <release>8</release>
                    <encoding>UTF-8</encoding>
                    <!--Use this comments args when compiler is NullPointerException-->
                    <!--<forceJavacCompilerUse>true</forceJavacCompilerUse>-->
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-shade-plugin</artifactId>
                <version>3.3.0</version>
                <configuration>
                    <finalName>QuickShop</finalName>
                    <minimizeJar>true</minimizeJar>
                    <createDependencyReducedPom>false</createDependencyReducedPom>
                    <createSourcesJar>false</createSourcesJar>
                    <createTestSourcesJar>false</createTestSourcesJar>
                    <relocations>
                        <relocation>
                            <pattern>com.google.errorprone.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.com.google.errorprone.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>org.checkerframework.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.org.checkerframework.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>org.apache.commons.lang3.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.org.apache.commons.lang3.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>org.objectweb.asm.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.org.objectweb.asm.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>org.slf4j.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.org.slf4j.slf4j.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>net.minidev.json.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.net.minidev.json.minidev.json.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>com.fasterxml.jackson.core.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.com.fasterxml.jackson.jackson.core.
                            </shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>me.lucko.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.me.lucko.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>com.rollbar.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.com.rollbar.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>com.dumptruckman.bukkit.configuration.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.com.dumptruckman.bukkit.configuration.
                            </shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>org.apache.commons.compress.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.org.apache.commons.compress.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>org.apache.commons.codec.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.org.apache.commons.codec.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>me.lucko.helper.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.me.lucko.helper.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>org.slf4j.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.org.slf4j.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>com.github.SparklingComet.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.com.github.SparklingComet.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>de.themoep.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.de.themoep.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>io.papermc.lib.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.io.papermc.lib.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>com.github.ben-manes.caffeine.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.com.github.ben-manes.caffeine.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>org.enginehub.squirrelid.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.org.enginehub.squirrelid.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>kotlin.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.org.kotlin.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>javax.annotation.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.org.javax.annotation.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>com.flowpowered.math.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.com.flowpowered.math.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>okio.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.okio.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>okhttp3.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.okhttp3.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>net.kyori.minecraft.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.net.kyori.minecraft.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>net.minidev.asm.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.net.minidev.asm.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>com.squareup.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.org.com.squareup.</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>de.leonhard.storage.</pattern>
                            <shadedPattern>org.maxgamer.quickshop.shade.de.leonhard.storage.</shadedPattern>
                        </relocation>
                    </relocations>
                    <transformers>
                        <transformer
                                implementation="org.apache.maven.plugins.shade.resource.ManifestResourceTransformer">
                            <mainClass>org.maxgamer.quickshop.javaw.bootstrap.Bootstrap</mainClass>
                        </transformer>
                    </transformers>
                    <filters>
                        <filter>
                            <artifact>*:*</artifact>
                            <excludes>
                                <exclude>META-INF/*.SF</exclude>
                                <exclude>META-INF/*.DSA</exclude>
                                <exclude>META-INF/*.RSA</exclude>
                                <exclude>META-INF/*.kotlin_module</exclude>
                                <exclude>META-INF/*.txt</exclude>
                                <exclude>META-INF/proguard/*</exclude>
                                <exclude>META-INF/services/*</exclude>
                                <exclude>META-INF/versions/9/*</exclude>
                                <exclude>*License*</exclude>
                                <exclude>*LICENSE*</exclude>
                            </excludes>
                        </filter>
                    </filters>
                </configuration>
                <executions>
                    <execution>
                        <phase>package</phase>
                        <goals>
                            <goal>shade</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-javadoc-plugin</artifactId>
                <version>3.4.1</version>
                <configuration>
                    <encoding>UTF-8</encoding>
                    <excludePackageNames>org.maxgamer.quickshop.nonquickshopstuff.*</excludePackageNames>
                    <!--<aggregate>true</aggregate>
                    <charmvnset>UTF-8</charmvnset>
                    <docencoding>UTF-8</docencoding>-->
                </configuration>
                <executions>
                    <execution>
                        <id>attach-javadocs</id>
                        <goals>
                            <goal>jar</goal>
                        </goals>
                        <configuration>
                            <failOnError>false</failOnError>
                            <additionalJOption>-Xdoclint:none</additionalJOption>
                        </configuration>
                    </execution>
                </executions>
            </plugin>

            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-jarsigner-plugin</artifactId>
                <version>3.0.0</version>
                <executions>
                    <execution>
                        <phase>${mvn.sign.phrase}</phase>
                        <id>sign</id>
                        <goals>
                            <goal>sign</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <archive>${project.basedir}/target/QuickShop.jar</archive>
                    <keystore>${project.basedir}/keystore.jks</keystore>
                    <alias>selfsigned</alias>
                    <arguments>
                        <argument>-signedjar</argument>
                        <argument>target/QuickShop-signed.jar</argument>
                    </arguments>
                    <!--suppress UnresolvedMavenProperty -->
                    <storepass>${env.quickshop-signer-pwd}</storepass>
                    <!--suppress UnresolvedMavenProperty -->
                    <keypass>${env.quickshop-signer-pwd}</keypass>
                </configuration>
            </plugin>
        </plugins>
        <resources>
            <resource>
                <targetPath>.</targetPath>
                <filtering>true</filtering>
                <directory>src/main/resources</directory>
                <excludes>
                    <exclude>src/main/resources/bin</exclude>
                    <exclude>src/main/resources/lang</exclude>
                </excludes>
            </resource>
            <resource>
                <targetPath>bin</targetPath>
                <filtering>false</filtering>
                <directory>src/main/resources/bin</directory>
            </resource>
            <resource>
                <targetPath>lang</targetPath>
                <filtering>false</filtering>
                <directory>src/main/resources/lang</directory>
            </resource>
            <resource>
                <targetPath>crowdin/lang</targetPath>
                <filtering>false</filtering>
                <directory>crowdin/lang</directory>
            </resource>
        </resources>
    </build>

